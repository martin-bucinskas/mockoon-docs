{
  "name": "mockoon-docs",
  "version": "0.1.7",
  "description": "GitHub Action for creating Mockoon documentation",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare": "ncc build index.js -o dist --source-map --license licenses.txt && husky install",
    "test": "jest",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martin-bucinskas/mockoon-docs"
  },
  "keywords": [
    "Mockoon",
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "Martin Bucinskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martin-bucinskas/mockoon-docs/issues"
  },
  "homepage": "https://github.com/martin-bucinskas/mockoon-docs",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@octokit/rest": "^20.0.1",
    "node-stringbuilder": "^2.2.6"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.48.0",
    "husky": "^8.0.0",
    "jest": "^29.6.4",
    "line-ending-corrector": "^1.0.1"
  }
}
